<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <inset
            android:insetTop="-1dp"
            android:insetBottom="-1dp"
            >
            <shape android:shape="rectangle">
                <solid android:color="@color/white" />
